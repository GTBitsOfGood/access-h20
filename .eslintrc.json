{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "project": ["./tsconfig.json"],
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-underscore-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "no-console": "off",
    "import/prefer-default-export": "off"
  }
}
